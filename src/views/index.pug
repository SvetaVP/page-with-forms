                    div.menu.col-xs-12.col-sm-6.col-lg-2
                        p.footer__titel MENU
                        ul
                            li
                                a(href="#") Objects
                            li
                                a(href="#") Hotels
                            li
                                a(href="#") Pakages
                            li
                                a(href="#") Events
